<div id="signupform">
  <h1 id="signuptext">
    Unleash Your Seasoning
  </h1>
  <%= form_for :user, url: users_path do |user| %>
    <%= user.text_field :username, :placeholder => "Username", :id => "signupUsername" %>
    <%= user.text_field :password, :placeholder => "Password", :id => "signupPassword" %>
    <%= user.submit "Sign Up", :id => "signupSubmit" %>
  <% end %>
</div>
<%=image_tag("salty_land.png", :class => "saltyL") %>
