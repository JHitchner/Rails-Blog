<div class="posteditform">
  <%= form_for :post, url: posts_path do |post| %>
    <p id="haters"> Salt Thy Haters</p>
    <p>
      <%= post.text_field :title, :placeholder => "Title", :class=> "postTitle" %>
    </p>
    <p>
      <%= post.text_area :content, :placeholder => "Body", :class=> "postBody" %>
    </p>
    <p>
      <%= post.submit "Post", :class => "postSubmit"  %>
    </p>
  <% end %>
  </div>
  <%= link_to 'Back to Account',user_path(session[:user_id]), :class => "backUser" %>
