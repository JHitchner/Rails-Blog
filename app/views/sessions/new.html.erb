<h1 id="sessLogin">Welcome Back</h1>
<div id="sessLoginForm">
 <%= form_for(:session, url: '/login' ) do |form| %>
    <%= form.text_field :username,  :placeholder => "Username", :id => "sessusername"%>
    <%= form.password_field :password,  :placeholder => "Password", :id => "sesspassword" %>
    <%= form.submit "Let's Get Salty", :id =>"sessSubmit" %>
 <% end %>
</div>

<p id="notmember">
  Not a member?
  <a href="/users/new">SignUp here!</a>
</p>
<%=image_tag("salty_land.png", :class => "saltyL") %>
